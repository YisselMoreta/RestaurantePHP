{% extends 'base.html.twig' %}

{% block body %}
   
{{ form_start(form) }}
{{ form_row(form._token) }}
    <div class="d-flex flex-row" id="registroContainer">
     <form  >
  <div class="form-group " >
      {{ form_row(form.username) }}
  </div>
  
  <div class="form-group ">
    {{ form_row(form.plainPassword.first) }}
  </div>
  <div class="form-group ">
    {{ form_row(form.plainPassword.second) }}
  </div>
  
  <div class="form-group ">
  
  <button type="submit" id="registroBoton" class="btn btn-primary ">Eniar</button>
  </div>
</form>
</div>
{{ form_rest(form) }}
{{ form_end(form) }}
   
   
{% endblock %}